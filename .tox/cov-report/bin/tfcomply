#!/Users/rubelw/projects/wills-github/terraform-compliance/.tox/cov-report/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'terraform-compliance','console_scripts','tfcomply'
__requires__ = 'terraform-compliance'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('terraform-compliance', 'console_scripts', 'tfcomply')()
    )
